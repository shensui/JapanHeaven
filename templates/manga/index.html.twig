{% extends 'manga/manga_base.html.twig' %}

{% block breadcrump %}
    <li class="breadcrumb-item active" aria-current="page"><a href="{{ path('manga_index') }}">Manga</a></li>
{% endblock %}

{% block manga_body %}
    <div class="manga_index">
        <ul class="nav nav-pills nav-fill" id="Tab_Manga" role="tablist">
            <li class="nav-item">
                <a class="nav-link active"  id="home-tab-num" data-toggle="tab" href="#tab_num" role="tab" aria-controls="home" aria-selected="true">#</a>
            </li>
            {% for item in az %}
                <li class="nav-item">
                    <a class="nav-link"  id="home-tab-{{ item }}" data-toggle="tab" href="#tab_{{ item }}" role="tab" aria-controls="home" aria-selected="true">{{ item|upper }}</a>
                </li>
            {% endfor %}
            <li class="nav-item">
                <a class="nav-link" href="{{ path('manga_new') }}"><i class="fas fa-plus"></i></a>
            </li>
        </ul>
