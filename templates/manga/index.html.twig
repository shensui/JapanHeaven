            <div class="tab-pane fade show active" id="tab_num" role="tabpanel" aria-labelledby="num">
                <div class="row">
                    {% if num|length >= 1 %}
                        {% for item in num %}
                            <div class="col-md-4">
                                <div class="card mb-4 shadow-sm">
                                    <img class="card-img-top" data-src="{{ asset(item.cover.WebPath) }}" alt="{{ item.cover.alt}}" style="height: 225px; width: 100%; display: block;" src="{{ asset(item.cover.WebPath) }}" data-holder-rendered="true">
                                    <div class="card-body">
                                        <p class="card-text">
                                            {{ item.title }}
                                            <br>
                                        </p>
                                        <div class="d-flex justify-content-between align-items-center">
                                            <div class="btn-group">
                                                <a href="{{ path('manga_show', {'id': item.id, 'slug': item.slug}) }}" class="btn btn-sm btn-outline-secondary">View</a>
                                                <a href="{{ path('manga_edit', {'id': item.id, 'slug': item.slug}) }}" class="btn btn-sm btn-outline-secondary">Edit</a>
                                            </div>
                                            <small class="text-muted">{{ item.interval() }}</small>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        {% endfor %}
                    {% else %}
                        <div class="col-md-12">
                            <p>Il n'y a pas encore d'élément validé pour cette lettre.</p>
                        </div>
